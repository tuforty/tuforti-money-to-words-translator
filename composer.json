{
  "name": "tuforti/money-to-words-translator",
  "version": "3.2.1",
  "description": "A php library that converts any money value in digit in any language or numeric system to its words in any language",
  "type": "library",
  "authors": [
    {
      "name": "Tochukwu Nkemdilim",
      "email": "nkemdilimtochukwu@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.1",
    "google/cloud-translate": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "Tuforti\\MoneyToWords\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "mockery/mockery": "^1.3"
  }
}